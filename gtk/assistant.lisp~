(in-package :gtk-cffi)

(defclass assistant (window)
  ())

(defcfun gtk-assistant-new :pointer)

(defmethod gconstructor ((assistant assistant) &key &allow-other-keys)
  (gtk-assistant-new))

(defslots assistant
  current-page :int
  )